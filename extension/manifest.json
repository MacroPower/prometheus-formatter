{
  "name": "Prometheus Formatter",
  "version": "1.2.4",
  "manifest_version": 2,
  "description": "Makes plain Prometheus metrics easier to read.",
  "homepage_url": "https://github.com/fhemberger/prometheus-formatter",
  "minimum_chrome_version": "60",
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_popup": "html/popup.html",
    "default_icon": {
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "prometheus-formatter@frederic-hemberger.de",
      "strict_min_version": "72.0"
    }
  }
}
